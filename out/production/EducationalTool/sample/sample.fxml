<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <AnchorPane fx:id="rootPane" maxHeight="-1.0" maxWidth="-1.0" onMouseEntered="#appAcessed" prefHeight="-1.0" prefWidth="-1.0" stylesheets="@Quiz.css" VBox.vgrow="ALWAYS">
            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ddfff7" height="608.0" stroke="#ddfff7" strokeType="INSIDE" width="800.0" />
                <Button alignment="CENTER" layoutX="322.0" layoutY="138.0" mnemonicParsing="false" onAction="#startButton" prefHeight="67.0" prefWidth="156.0" text="Start" textFill="WHITE">
                    <font>
                        <Font name="Curlz MT" size="20.0" />
                    </font></Button>
                <Button alignment="CENTER" layoutX="322.0" layoutY="247.0" mnemonicParsing="false" onAction="#settingsButton" prefHeight="61.0" prefWidth="156.0" text="Options" textFill="WHITE">
                    <font>
                        <Font name="Curlz MT" size="20.0" />
                    </font></Button>
                <Button alignment="CENTER" layoutX="322.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#infoButton" prefHeight="67.0" prefWidth="156.0" text="Info" textFill="WHITE">
                    <font>
                        <Font size="20.0" />
                    </font></Button>
                <Button alignment="CENTER" layoutX="322.0" layoutY="450.0" mnemonicParsing="false" onAction="#quizButton" prefHeight="67.0" prefWidth="156.0" text="Quiz" textFill="WHITE">
                    <font>
                        <Font name="Curlz MT" size="20.0" />
                    </font></Button>
                <Text fill="#114b5f" layoutX="119.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Educational Tool For Robotics">
                    <font>
                        <Font name="Curlz MT" size="45.0" />
                    </font>
                </Text>
            <Rectangle fx:id="Tint" arcHeight="5.0" arcWidth="5.0" disable="true" fill="WHITE" height="608.0" opacity="0.2" stroke="#ddfff7" strokeType="INSIDE" visible="false" width="800.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
