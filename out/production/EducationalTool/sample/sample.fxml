<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <AnchorPane fx:id="rootPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="startButton" alignment="CENTER" layoutX="322.0" layoutY="138.0" mnemonicParsing="false" onAction="#startButton" onMouseEntered="#startButtonMouseOver" prefHeight="67.0" prefWidth="156.0" text="Start" />
                <Button fx:id="settingsButton" layoutX="322.0" layoutY="247.0" mnemonicParsing="false" onAction="#settingsButton" onMouseEntered="#settingsButtonMouseOver" prefHeight="61.0" prefWidth="156.0" text="Settings" />
                <Button fx:id="aboutButton" layoutX="322.0" layoutY="342.0" mnemonicParsing="false" onAction="#infoButton" onMouseClicked="#infoButton" onMouseEntered="#aboutButtonMouseOver" prefHeight="67.0" prefWidth="156.0" text="Info" />
                <Button fx:id="quizButton" alignment="CENTER" layoutX="322.0" layoutY="450.0" mnemonicParsing="false" onAction="#quizButton" onMouseEntered="#quizButtonMouseOver" prefHeight="67.0" prefWidth="156.0" text="Quiz" />
                <Text layoutX="119.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Educational Tool For Robotics">
                    <font>
                        <Font size="45.0" />
                    </font>
                </Text>
            </children>
        </AnchorPane>
    </children>
</VBox>
